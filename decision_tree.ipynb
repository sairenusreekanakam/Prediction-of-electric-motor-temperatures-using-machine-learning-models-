import pandas as pd
import plotly.express as px
from plotly.subplots import make_subplots
import plotly.graph_objects as go
from sklearn.impute import SimpleImputer
import numpy as np
from sklearn.ensemble import RandomForestClassifier
from  sklearn.preprocessing import OrdinalEncoder
from sklearn.tree import DecisionTreeRegressor
from sklearn.model_selection import train_test_split
from sklearn.model_selection import cross_validate
from sklearn.model_selection import StratifiedKFold
from  sklearn.metrics import *
import matplotlib.pyplot as plt
from sklearn.preprocessing import StandardScaler
from sklearn import tree
from sklearn import model_selection
     

df_test=pd.read_csv('/content/gdrive/My Drive/Data Science/Electric_motor_temperature/01_Data/02_Clean/test.csv')
df_train=pd.read_csv('/content/gdrive/My Drive/Data Science/Electric_motor_temperature/01_Data/02_Clean/train.csv')

inputs=['ambient', 'coolant', 'u_d', 'u_q', 'speed', 'i_d', 'i_q']    #identify the input features
outputs=['pm','yoke', 'tooth', 'winding']              #identify the output features

#split into X and y data
X_test=df_test[inputs].reset_index(drop=True) 
y_test=df_test[outputs].reset_index(drop=True) 

X_train=df_train[inputs].reset_index(drop=True) 
y_train=df_train[outputs].reset_index(drop=True)
     

def MSE(predic,ref):
  diff=(predic-ref)**2
  mean_diff=diff.mean()
  return mean_diff
     
Decision tree model with the default parameters


decision_tree_max=DecisionTreeRegressor()
decision_tree_max.fit(X_train,y_train)
y_pred_max=decision_tree_max.predict(X_test)
y_pred_max=pd.DataFrame(y_pred_max,columns=y_test.columns)
     
Decision tree with a maximum depth of 5


decision_tree_5=DecisionTreeRegressor(max_depth=5)
decision_tree_5.fit(X_train,y_train)
y_pred_5=decision_tree_5.predict(X_test)
y_pred_5=pd.DataFrame(y_pred_5,columns=y_test.columns)
     

decision_tree_graph = make_subplots(rows=2, cols=2,subplot_titles=["pm","yoke","tooth","winding"])
decision_tree_graph.add_trace(go.Scatter(y=y_pred_max.pm,line={'color':'teal'},name="max_depth=none"),row=1, col=1)
decision_tree_graph.add_trace(go.Scatter(y=y_pred_5.pm,line={'color':'coral'},name="max_depth=5"),row=1, col=1)
decision_tree_graph.add_trace(go.Scatter(y=y_test.pm,line={'color':'black'},name="real values"),row=1, col=1)

decision_tree_graph.add_trace(go.Scatter(y=y_pred_max.yoke,line={'color':'teal'},showlegend=False),row=1, col=2)
decision_tree_graph.add_trace(go.Scatter(y=y_pred_5.yoke,line={'color':'coral'},showlegend=False),row=1, col=2)
decision_tree_graph.add_trace(go.Scatter(y=y_test.yoke,line={'color':'black'},showlegend=False),row=1, col=2)

decision_tree_graph.add_trace(go.Scatter(y=y_pred_max.tooth,line={'color':'teal'},showlegend=False),row=2, col=1)
decision_tree_graph.add_trace(go.Scatter(y=y_pred_5.tooth,line={'color':'coral'},showlegend=False),row=2, col=1)
decision_tree_graph.add_trace(go.Scatter(y=y_test.tooth,line={'color':'black'},showlegend=False),row=2, col=1)

decision_tree_graph.add_trace(go.Scatter(y=y_pred_max.winding,line={'color':'teal'},showlegend=False),row=2, col=2)
decision_tree_graph.add_trace(go.Scatter(y=y_pred_5.winding,line={'color':'coral'},showlegend=False),row=2, col=2)
decision_tree_graph.add_trace(go.Scatter(y=y_test.winding,line={'color':'black'},showlegend=False),row=2, col=2)
decision_tree_graph.update_layout(title_text='Comparison of predicted values vs measured values',legend_orientation="h")
decision_tree_graph.show()
     
